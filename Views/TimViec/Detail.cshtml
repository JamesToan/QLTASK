                                            </div>
                                            <div class="divInfoHalf"></div>
                                        </section>
                                        <section class="divProfile size-095rem">
                                            <div class="title-section">
                                                <span>
                                                    <i class="fal fa-user-alt"></i> Thông tin hồ sơ
                                                </span>
                                            </div>
                                            <div class="row infoHoSo">
                                                <div class="col-sm-7">
                                                    <p>
                                                        <span class="fw-500">
                                                            Mức lương mong muốn:
                                                        </span>
                                                        @{
                                                            if (timviec.MucLuongTu == null && timviec.MucLuongDen == null)
                                                            {
                                                                <span>Thỏa Thuận</span>
                                                            }
                                                            else if (timviec.MucLuongTu != null && timviec.MucLuongDen == null)
                                                            {
                                                                <span> Trên @timviec.MucLuongTu triệu</span>
                                                            }
                                                            else if (timviec.MucLuongTu != null && timviec.MucLuongDen != null)
                                                            {
                                                                <span>
                                                                    Từ @timviec.MucLuongTu - @timviec.MucLuongDen triệu
                                                                </span>
                                                            }
                                                        }

                                                    </p>
                                                    <p>
                                                        <span class="fw-500">
                                                            Kinh nghiệm:
                                                        </span>
                                                        @{
                                                            if (timviec.KinhNghiem != null)
                                                            {
                                                                @timviec.KinhNghiem
                                                            }
                                                            else
                                                            {
                                                                <span>Không xác định</span>

                                                            }
                                                        }

                                                    </p>
                                                    @*<p>
                                                            <span class="fw-500">
                                                                Lượt xem:
                                                            </span>
                                                            517
                                                        </p>*@
                                                    <p>
                                                        <span class="fw-500">Ngành nghề: </span>
                                                        @{
                                                            foreach (var item in nganhnghe)
                                                            {
                                                                if (item.Id == timviec.NganhNgheId)
                                                                {
                                                                    <span class="badge badge-info">
                                                                        @item.TenNganhNghe
                                                                    </span>
                                                                }
                                                            }
                                                        }
                                                        @*<span class="badge badge-info">
                                                                Hành chính / Thư ký / Văn phòng
                                                            </span>
                                                            <span class="badge badge-info">
                                                                Kinh doanh
                                                            </span>
                                                            <span class="badge badge-info">
                                                                Kế toán / Tài chính / Kiểm toán
                                                            </span>
                                                            <span class="badge badge-info">
                                                                Thực phẩm / Dịch vụ ăn uống
                                                            </span>*@
                                                    </p>
                                                </div>
                                                <div class="col-sm-5">
                                                    @{
                                                        for (int i = 0; i < vitri.Count; i++)
                                                        {
                                                            if (vitri[i].Id == timviec.ViTriCongViecId)
                                                            {

                                                                <p>
                                                                    <span class="fw-500">
                                                                        Chức vụ mong  muốn:
                                                                    </span>
                                                                    @vitri[i].TenViTriCongViec
                                                                </p>
                                                            }
                                                        }

                                                        foreach (var item in vanBang)
                                                        {
                                                            if (timviec.VanBangId == item.Id)
                                                            {
                                                                <p>
                                                                    <span class="fw-500">
                                                                        Trình độ học vấn:
                                                                    </span>
                                                                    @item.TenVanBang
                                                                </p>
                                                            }
                                                        }

                                                    }


                                                    @*<p>
                                                            <span class="fw-500">
                                                                Ngày cập nhật:
                                                            </span>
                                                            @{
                                                                var date = timviec.NgayTao.ToString().Split(' ');
                                                                var time = date[0];
                                                                @time
                                                            }

                                                        </p>*@
