                                        <section class="divTimeLine size-095rem">




                                            <div class="title-section mb-0">
                                                <span>
                                                    <i class="fal fa-graduation-cap"></i> Học vấn / bằng cấp
                                                </span>
                                            </div>

                                            @{
                                                foreach (var item in vanhoa)
                                                {
                                                    if (timviec.VanHoaId == item.Id)
                                                    {

                                                        <div class="timeline">

                                                            <div class="item">
                                                                <div class="itemTime">

                                                                    <p><span class="fw-500">Văn hóa: </span></p>

                                                                </div>
                                                                <div class="itemInfo">
                                                                    <p><span class="fw-500">@item.TenVanHoa</span></p>

                                                                </div>
                                                            </div>
                                                        </div>


                                                    }
                                                }
                                            }



                                            @{
                                                foreach (var item in vanBang)
                                                {
                                                    if (timviec.VanBangId == item.Id)
                                                    {
                                                        <div class="timeline">

                                                            <div class="item">
                                                                <div class="itemTime">

                                                                    <p><span class="fw-500">Bằng cấp: </span></p>

                                                                    <p>
                                                                        <span class="fw-500">Chuyên ngành:</span>
                                                                    </p>
                                                                </div>
                                                                <div class="itemInfo">
                                                                    <p><span class="fw-500">@item.TenVanBang</span></p>

                                                                    <p><span class="fw-500">@timviec.ChuyenNganh</span></p>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    }
                                                }
                                            }

                                        </section>
                                        <section class="divTimeLine size-095rem">
                                            <div class="title-section mb-0">
                                                <span>
                                                    <i class="fal fa-briefcase"></i> Kỹ năng
                                                </span>
                                            </div>

                                            <div class="timeline">
                                                <div class="item">
                                                    <div class="itemTime">
                                                        Ngoại ngữ:
                                                    </div>
                                                    <div class="itemInfo">
                                                        @{
                                                            if (timviec.NgoaiNguId != null)
                                                            {
                                                                <p>
                                                                    <span class="fw-500">@ngoaingu.TenChungChi</span>
                                                                </p>
                                                            }
                                                            else
                                                            {
                                                                <p>
                                                                    <span class="fw-500">Không</span>
                                                                </p>
                                                            }
                                                        }


                                                    </div>
                                                </div>
                                            </div>
                                            <div class="timeline">
                                                <div class="item">
                                                    <div class="itemTime">
                                                        Tin học:
                                                    </div>
                                                    <div class="itemInfo">
                                                        @{
                                                            if (timviec.TinHocId != null)
                                                            {
                                                                <p>
                                                                    <span class="fw-500">@tinhoc.TenChungChi</span>
                                                                </p>
                                                            }
                                                            else
                                                            {
                                                                <p>
                                                                    <span class="fw-500">Không</span>
                                                                </p>
                                                            }
                                                        }



                                                    </div>
                                                </div>
                                            </div>
                                            <div class="timeline">
                                                <div class="item">
                                                    <div class="itemTime">
                                                        Kỹ năng khác:
                                                    </div>
                                                    <div class="itemInfo">
                                                        @{
                                                            if (timviec.KyNang == null)
                                                            {
                                                                <p><span class="fw-500">Không</span></p>
                                                            }
                                                            else
                                                            {
                                                                <p><span class="fw-500">@timviec.KyNang</span></p>
                                                            }
                                                        }


                                                    </div>
                                                </div>
                                            </div>

                                        </section>

                                    </div>
                                }
                            }



                            <div class="mt-2 mb-2">
